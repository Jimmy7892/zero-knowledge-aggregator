{
  "name": "track-record-enclave",
  "version": "1.0.0",
  "description": "Track Record Platform - Enclave Worker (AMD SEV-SNP) - Open Source Auditable Code",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/track-record-enclave"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:web": "ts-node scripts/test/test-server.ts",
    "lint": "eslint src --ext .ts",
    "prisma:generate": "prisma generate",
    "docker:build": "docker-compose -f docker-compose.dev.yml build",
    "docker:up": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:logs:enclave": "docker logs -f enclave_worker_dev",
    "docker:restart": "docker-compose -f docker-compose.dev.yml restart enclave",
    "docker:rebuild": "docker-compose -f docker-compose.dev.yml up --build -d enclave",
    "docker:shell": "docker exec -it enclave_worker_dev sh",
    "docker:clean": "docker-compose -f docker-compose.dev.yml down -v",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:push:docker": "docker exec enclave_worker_dev npx prisma db push"
  },
  "keywords": [
    "trading",
    "enclave",
    "sev-snp",
    "confidential-computing",
    "zero-knowledge",
    "auditable"
  ],
  "author": "Track Record Platform",
  "license": "MIT",
  "dependencies": {
    "@alpacahq/alpaca-trade-api": "^3.1.3",
    "@grpc/grpc-js": "^1.14.1",
    "@grpc/proto-loader": "^0.8.0",
    "@prisma/client": "^6.19.0",
    "@types/xml2js": "^0.4.14",
    "axios": "^1.11.0",
    "ccxt": "^4.5.22",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.0.0",
    "node-cron": "^4.2.1",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "xml2js": "^0.6.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "prisma": "^6.19.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
